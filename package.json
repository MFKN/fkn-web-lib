{
  "name": "@banou26/oz-lib",
  "version": "0.0.1",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@apollo/client": "^3.3.16",
    "esbuild": "^0.13.3",
    "nodemon": "^2.0.13",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build2": "rollup -c",
    "dev2": "rollup -c -w",
    "dev3": "tsc --watch",
    "type": "tsc",
    "build3": "parcel build ./src/index.ts ./src/service-worker/index.ts && tsc",
    "build": "esbuild ./src/index.ts --format=esm --bundle --outfile=lib/index.js && tsc",
    "dev": "nodemon --watch src -e ts --exec \"npm run build\""
  }

{
  "name": "@banou26/oz-lib",
  "version": "0.0.1",
  "main": "lib/lib/src/index.js",
  "type": "module",
  "license": "MIT",
  "devDependencies": {
    "@apollo/client": "^3.3.16",
    "parcel": "^2.0.0-nightly.753",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "build2": "rollup -c",
    "dev2": "rollup -c -w",
    "dev": "tsc --watch",
    "build": "parcel build ./src/index.ts && tsc"
  },
  "types": "lib/lib/src/index.d.ts",
  "targets": {
    "main": {
      "isLibrary": true,
      "outputFormat": "esmodule",
      "includeNodeModules": [
        "@mfkn/oz-web"
      ]
    }
  },
  "alias": {
    "@mfkn/oz-web": "../web"
  }
}
